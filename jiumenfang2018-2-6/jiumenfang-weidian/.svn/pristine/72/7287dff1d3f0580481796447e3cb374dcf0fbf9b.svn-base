.wrap .shopping-pop-cart {
  width: 100%;
  height:23.35rem;
 /*  padding: 6rem 0; */
  background-color: #fff;
  margin: 0 auto;
  position: absolute;
  bottom: 0;
  z-index:10000;
}

.wrap .shopping-pop-cart .pop-name {
	width:39.33%;
	text-align:center;
	padding-top:10.68%;
	float:left;
}
.wrap .shopping-pop-cart .pop-name img {
	width:50.8%;
}
.wrap .shopping-pop-cart .pop-num .pop-nam {
  font-size: 2.4rem;
  position: absolute;
  top: 11.75%;
}
.wrap .shopping-pop-cart .pop-num{
	width:50%;
	padding-top:10%;
	float:left;
}
 .wrap .shopping-pop-cart .pop-num-nameWrap{
	margin-top:22%;
}
.wrap .shopping-pop-cart .pop-priceWrap,.wrap .shopping-pop-cart .pop-shipmentsWrap{
	margin-top:3%;
}
 .wrap .shopping-pop-cart .pop-num-name, .wrap .shopping-pop-cart .pop-price-name, .wrap .shopping-pop-cart .pop-shipments-name{
	font-size:1.8rem;
	color:#000;
	opacity:0.6;
	filter:alpha(opacity=60);
	float:left;
} 
.wrap .shopping-pop-cart .pop-num .pop-change p {
  float: right;
  font-size:1.8rem;
  border-radius:0.4rem;
}
.pop-change	.pop-change-subtract{
	background-color:#a7a7a7;
	color:#fff;
	padding: 0 0.8rem;
}
.pop-change	.pop-change-add{
	background-color:#ff4891;
	color:#fff;
	padding: 0 0.6rem;
}
.pop-change	.pop-change-num{
	padding: 0 0.6rem;
}
.wrap .shopping-pop-cart .pop-priceWrap .pop-price,.wrap .shopping-pop-cart .pop-shipmentsWrap .pop-shipments-time {
  float:right;
  font-size:1.5rem;
  padding-top:0.5rem;
}
.wrap .shopping-pop-cart .pop-footer {
  width: 100%;
  padding: 1.1rem 0;
  background-color: #FF4689;
  color: #fff;
  font-size: 1.8rem;
  text-align: center;
  position: absolute;
  bottom: 0;
  left:0;
}
