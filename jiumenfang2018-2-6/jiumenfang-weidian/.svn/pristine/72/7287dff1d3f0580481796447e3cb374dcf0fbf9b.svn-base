html,
body {
  font-size: 10px;
}
.wrap {
  width: 100%;
  font-size: 1.6rem;
  position: relative;
  background-color:#f3f3f3;	
}
.wrap .single-con {
  width: 100%;
  margin: 0 auto 4.9rem;
}
.wrap .single-con .single-header {
  position: relative;
}
.wrap .single-con .single-header span {
  position: absolute;
  top: 0.5rem;
  right: 1rem;
}
.wrap .single-con .single-banner img {
  width: 100%;
}
.wrap .single-con .single-wrap{
	width:100%;
	margin:0 auto;
}
.wrap .single-con .single-info {
  width: 90%;
  padding:0.8rem 0;
  box-shadow: 0 0 1rem #efefef;
  border-radius:0.4rem;
  background-color:#fff;
  margin:0	auto;
}
.wrap .single-con .single-info	.single-infoin{
	width:95%;
	border:1px solid #f8f8f8;
	box-shadow: 0 0 1px #ececec;
	border-radius:0.4rem;
	margin:0 auto;
} 
.single-infoin	.single-in{
	width:90%;
	margin:4% auto;
	border-bottom:1px solid	#ffa3c8;
}
.single-in .single-price{
	margin-bottom:1.9rem;
}
.single-in .info-name{
	font-size:1.6rem;
	margin-bottom:1.25rem;
}

.wrap .single-con .single-list {
  width: 100%;
  margin-top:2rem;
}
.wrap .single-con .single-list .tab-nav {
	width:60%;
  	margin:0 auto 1.5rem;
}
.wrap .single-con .single-list .tab-nav li {
  width: 32%;
  float: left;
  font-size:1.6rem;
  color:#000;
  opacity:0.5;
  filter:alpha(opacity=50);
  padding: 0.8rem 0 0.2rem;
  text-align: center;
  cursor: pointer;
}
.wrap .single-con .single-list .tab-nav .tab-nav-active {
  border-bottom: 0.2rem solid #ff4891;
  color:#000;
   opacity:1;
  filter:alpha(opacity=50);
}
.wrap .single-con .single-list .tab-info {
  width: 100%;
  margin:0 auto;
}
 .single-list .tab-info li {
  width: 100%;
  font-size:1.6rem;
  color:#000;
  display: none;
}
.wrap .single-con .single-list .tab-info li p {
  margin-top: 1rem;
  font-size: 1.4rem;
  color: #ABABAB;
}
.wrap .single-con .single-list .tab-info li img {
  width: 100%;
}

.wrap .single-con .single-list .tab-info .tab-info-active {
  display: block;
}
.wrap .single-con .single-list .tab-info .tab-info-active .tab-info-info {
  width: 100%;
  margin-top: 0.5rem;
}
.wrap .single-con .single-list .tab-info .tab-info-active .tab-info-info div:nth-child(1) {
  margin-right: 2%;
}
.wrap .single-con .single-list .tab-info .tab-info-active .tab-info-info .tab-info-text {
  width: 48%;
  float: left;
  font-size: 1.4rem;
  color: #ABABAB;
}

.wrap .single-footer-wrap {
  width: 100%;
  height: 4.9rem;
  position: fixed;
  bottom: 0;
  z-index: 999;

}
.wrap .single-footer-wrap .single-footer {
	width:100%;
	height:100%;
	margin:0 auto;
    background-color: #fff;
}
/* .single-footer-wrap .single-footer .single-footerin{
	width:85%;
	margin:0 auto;
} */
.wrap .single-footer-wrap .single-footer .shopping-bag {
  width: 27%;
  height: 100%;
  float: left;
  position: relative;
}
.shopping-bag img{
	width:24%;
	position:absolute;
	top:50%;
	left:3rem;
	transform:translateY(-50%);
}
.wrap .single-footer-wrap .single-footer .shopping-bag .shopping-num {
  font-size: 1.2rem;
  position: absolute;
  top: 0.5rem;
  right: 3rem;
  color: #ff4891;
}
.wrap .single-footer-wrap .single-footer .shopping-bag-add {
  width: 29.3%;
  height: 66%;
  position:relative;
  top:50%;
  transform:translateY(-50%);
  border-radius:0.45rem;
  color:#fff;  
  text-align: center;
  line-height: 3.234rem;
  float: left;
  background-color:#ff4891;
}
.wrap .single-footer-wrap .single-footer .shopping-bag-settlement {
  width: 29.3%;
  height: 65%;
  position:relative;
  top:50%;
  transform:translateY(-50%);
  border:0.15rem solid #bfbfbf;
  border-radius:0.45rem;
  text-align: center;
  line-height: 3.234rem;
  margin-left:2.5rem;
  color: #000;
  float: left;
}
.wrap .top-back {
  width: 3rem;
  height: 3rem;
  border-radius: 50%;
  border: 2px solid #ccc;
  position: fixed;
  bottom: 6rem;
  right: 1rem;
  color: #ccc;
  text-align: center;
  line-height: 3rem;
}
.wrap .shopping-popWrap,.wrap .shopping-popWrap-cart{
	width:100%;
	background-color:rgba(0,0,0,0.3);	
	display: none;
	margin: 0 auto;
	position: absolute;
  	bottom: 0;
  	z-index: 999;
  	border-top:1px solis #e129d6;
}
.shopping-popWrap	.shopping-mask,.shopping-popWrap-cart	.shopping-mask-cart{
	width:100%;
	background:#000;
	opacity:0.5;
	filter:alpha(opacity=50);
	}
