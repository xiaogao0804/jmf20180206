.wrap {
  width: 100%;
  background-color: #F2F5f5;
}
.wrap .contain {
  width: 100%;
   position: relative;
  top:0.3rem;
}
.wrap .contain .header {
  width: 100%;
  position: relative;
  text-align: center;
  line-height: 0.7rem;
  top: 0.2rem;
  color: #000;
}
.wrap .contain .header .fa-long-arrow-left {
  color: #ABABAB;
  position: absolute;
  left: 0.2rem;
}
.wrap .contain .header .fa-remove {
  color: #ABABAB;
  position: absolute;
  top: 0.1rem;
  right: 0.1rem;
  font-size: 0.5rem;
}
.wrap .contain .con-list-wrap {
  width: 100%;
  background-color: #fff;
  margin-bottom:0.3rem;	
}
/* .wrap .contain .not-order{
  position: absolute;
  top:30%;
  left:50%;
  transform: translateX(-50%);
  color: #000;
  opacity:0.9;
} */
.wrap .contain .con-list-wrap .con-list {
  width: 95%;
  margin-left: 0.5rem;
  position: relative;
  padding-bottom: 0.5rem;
  top: 0.5rem;
}
.wrap .contain .con-list-wrap .con-list .con-list-item {
  padding-bottom: 0.5rem;
  font-size: 0.4rem;
  color: #000;
}
.wrap .contain .con-list-wrap .con-list .con-list-item .item-num {
  float: left;
}
.wrap .contain .con-list-wrap .con-list .item-opa{
  color: #000;
  opacity:0.9;
  filter:alpha(opacity=90);
  padding:0.5rem 0;
}
.wrap .contain .con-list-wrap .con-list .item-shu{
  float: right;
  margin-right: 0.5rem;
}
.wrap .contain .con-list-wrap .con-list .con-list-item .goods-name{
  font-size: 0.32rem;
  color: #000;
  font-weight: bold;
}
.wrap .contain .con-list-wrap .con-list .con-list-item .recipient {
  float: left;
  color: #000;
  opacity:0.9;
  filter:alpha(opacity=90);
}

.wrap .contain .con-list-wrap .con-list .con-list-item .tel {
  float: right;
  margin-right: 0.5rem;
}
.wrap .contain .con-list-wrap .con-list .con-list-item .return-money {
  float: right;
  margin-right: 0.5rem;
  padding:0.15rem 0.5rem;
  background-color: #858585;
  color: #fff;
  border-radius: 0.1rem;
}
.wrap .contain .con-list-wrap .con-list .con-list-item .item-price {
  float: right;
  margin-right: 0.5rem;
  color: red;
}
.wrap .contain .con-list-wrap .con-list .con-list-item .item-order {
  float: right;
  margin-right: 0.5rem;
  color: #000000;
}

.wrap .contain .con-list-wrap .con-list .pay-btn {
  font-size: 0.3733rem;
}
.wrap .contain .con-list-wrap .con-list .pay-btn .pay-state {
  float: left;
}
.wrap .contain .con-list-wrap .con-list .pay-btn .refund-btn {
  float: right;
  margin-right: 1rem;
  padding: 0.02rem 0.2rem;
  background-color: #CDCCC9;
}
.wrap .contain .con-list-wrap .con-list .shipments-btn {
  font-size: 0.3733rem;
}
.wrap .contain .con-list-wrap .con-list .shipments-btn .pay-state {
  float: left;
}
.wrap .contain .con-list-wrap .con-list .shipments-btn .refund-btn {
  float: right;
  margin-right: 1rem;
  padding: 0.02rem 0.2rem;
  background-color: #CDCCC9;
}
.wrap .contain .con-list-wrap .con-list .logistics-state {
  font-size: 0.3733rem;
  margin-top: 0.25rem;
  color: #A1A2A6;
}
.wrap .contain .mask-pop {
  width: 100%;
  background-color: rgba(0, 0, 0, 0.65);
  position: absolute;
  top: 0;
  left: 0;
  display: none;
}
.wrap .contain .mask-pop .pop-info {
  width: 5rem;
  border: 1px solid #7E7E7E;
  border-radius: 0.25rem;
  padding: 1rem 1rem;
  background-color: #fff;
  margin: 50% auto;
  text-align: center;
  font-size: 0.3733rem;
}
.wrap .contain .mask-pop .pop-info .pop-txt {
  margin-bottom: 1rem;
  color: #3C3333;
}
.wrap .contain .mask-pop .pop-info .pop-cancel {
  float: left;
  padding: 0.1rem 0.8rem;
  border-radius: 0.13rem;
  border: 1px solid #A0A0A0;
  background-color: #F4F4F4;
  color: #656567;
}
.wrap .contain .mask-pop .pop-info .pop-confirm {
  float: right;
  padding: 0.1rem 0.8rem;
  border-radius: 0.13rem;
  border: 1px solid #A0A0A0;
  background-color: #F4F4F4;
  color: #656567;
}
.wrap .contain .mask-pop .pop-del {
  width: 0.75rem;
  height: 0.75rem;
  border: 2px solid #fff;
  border-radius: 50%;
  text-align: center;
  line-height: 0.75rem;
  color: #fff;
  margin: 6rem auto 0;
}
