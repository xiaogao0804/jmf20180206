.wrap{
    width: 100%;
    position:relative;
}
.wrap .header{
    width:100%;
    height:100px;
    background-color: #000;
    vertical-align: 100px;
    position: fixed;
    z-index: 9999;
}
.header .logo-wrap{
	width:40%;
    float: left;
}
.header .tab-wrap{
	width:60%;
    float: right;
    position: relative;
    top:70%;
    transform: translateY(-50%); 
}
.tab-wrap .tab{
	width:500px;
	height:30px;
	float:right;
}
.header .header-wrap{
	width:60%;
	height:100px;
	margin:0 auto;
}
.tab-wrap .tab li {
    float: left;
    padding:3px 6%;	
}
@media screen and (max-width: 1200px) {
	.tab-wrap .tab{
	   width:290px;
	}
   .tab-wrap .tab li {
       padding:3px 3%;
   }	
}
.tab-wrap .tab li:nth-child(5){
	padding-right:0;
}
.tab-wrap .tab li a{
    color: #fff;
    font-size: 20px;
}
.tab-wrap .tab li a:hover{
    color: #f58466;
    text-decoration:none;
}

/*滚到上部*/
.top-wrap{
    position: fixed;
    bottom:30px;
    right:20px;
    cursor: pointer;
    z-index: 99999;
}
