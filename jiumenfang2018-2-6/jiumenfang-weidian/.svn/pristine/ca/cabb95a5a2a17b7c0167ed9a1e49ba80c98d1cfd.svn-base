.wrap{
  width:100%;
  .cart-bag-wrap{
    width:90%;
    margin: 0.3rem auto;
    .cart-bag-cancel{
      width:100%;
      color: #333;
    }
    .cart-bag-con{
      width:100%;
      p{
        margin:0.3rem 0;
        color: #333;
      }
      .cart-bag-list{
        width:90%;
        margin: 0.3rem auto;
        .cart-bag-item{
          width: 100%;
          height:3rem;
          border:1px solid #969696;
          .cart-bag-logo{
            float: left;
            width:20%;
            height:100%;
            border-right: 1px solid #000;
          }
          .cart-bag-txt{
            float: right;
            width:77%;
            p{
              margin:0.5rem 0 0.2rem 0.3rem;
              color: #333;
              font-size: 0.3733rem;
            }
           p:nth-child(2){
             font-size: 0.6933rem;
             font-weight: bold;
           }
          }
        }
      }
    }
  }
}